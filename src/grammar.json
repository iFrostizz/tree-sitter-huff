{
  "name": "huff",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "definition"
      }
    },
    "definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "macro_definition"
        },
        {
          "type": "SYMBOL",
          "name": "function_definition"
        },
        {
          "type": "SYMBOL",
          "name": "interface_definition"
        }
      ]
    },
    "_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "parameters"
        }
      ]
    },
    "_def": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_declaration"
        },
        {
          "type": "STRING",
          "value": " = "
        },
        {
          "type": "SYMBOL",
          "name": "takes_parameters"
        },
        {
          "type": "SYMBOL",
          "name": "returns_parameters"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        }
      ]
    },
    "_def_params": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "visibility"
        },
        {
          "type": "SYMBOL",
          "name": "return_parameters"
        }
      ]
    },
    "def_val": {
      "type": "STRING",
      "value": "#define"
    },
    "macro_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#define macro "
        },
        {
          "type": "SYMBOL",
          "name": "_def"
        }
      ]
    },
    "function_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#define fn "
        },
        {
          "type": "SYMBOL",
          "name": "_def"
        }
      ]
    },
    "interface_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#define function "
        },
        {
          "type": "SYMBOL",
          "name": "_def_params"
        }
      ]
    },
    "return_parameters": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": " returns "
        },
        {
          "type": "SYMBOL",
          "name": "parameters"
        }
      ]
    },
    "takes_parameters": {
      "type": "PATTERN",
      "value": "takes\\s?[(][0-9]+[)]"
    },
    "returns_parameters": {
      "type": "PATTERN",
      "value": "returns\\s?[(][0-9]+[)]"
    },
    "block": {
      "type": "STRING",
      "value": "{}"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "([A-Za-z]+)"
    },
    "parameters": {
      "type": "PATTERN",
      "value": "[(](?:[A-Za-z0-9\\[\\],\\s]+)?[)]"
    },
    "visibility": {
      "type": "STRING",
      "value": "pure"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "//"
              },
              {
                "type": "PATTERN",
                "value": "([^\\r\\n])*"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "/*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\s\\uFEFF\\u2060\\u200B\\u00A0]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

